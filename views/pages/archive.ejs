<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/archiveheader.ejs %>
  </head>

  <body>
    <nav class="navbar">
      <label class="navbar-toggle" id="js-navbar-toggle" for="chkToggle">
              <i class="fa fa-bars"></i>
          </label>
      
      <input type="checkbox" id="chkToggle"></input>
      <ul class="main-nav" id="js-menu">
       
        <li>
          <a href="/" class="nav-links">Timer</a>
        </li>
        <li>
          <a href="/archive" class="nav-links">Archive</a>
        </li>
      </ul>
    </nav>
 <div class="wrapper">

  <div id="archive-container">
    <h3>Archive </h3>
    
    <ul id = "list" class = "archive-list">

      
      <% for ( let tomato of tomatos ) { %>
        <li class="<%= tomato.month %>">
          
          <%= tomato.date %>
            
        </li>
      <% } %>
     
    </ul>
    <div class="post-archive">
      
      <ul class="December-archive">
<h3 class="dec">December</h3>
</ul>

      <ul id="November-archive">
<h3 class="nov">November</h3>
</ul>
      
<ul id="October-archive">
<h3 class="oct">October</h3>
</ul>
      
      <ul id="September-archive">
<h3>September</h3>
</ul>

      <ul id="August-archive">
<h3>August</h3>
</ul>

      <ul id="July-archive">
<h3>July</h3>
</ul>

      <ul id="June-archive">
<h3>June</h3>
</ul>

      <ul id="May-archive">
<h3>May</h3>
</ul>

      <ul id="April-archive">
<h3>April</h3>
</ul>

      <ul id="March-archive">
<h3>March</h3>
</ul>

      <ul id="February-archive">
<h3>February</h3>
</ul>

      <ul id="January-archive">
<h3>January</h3>
</ul>

    </div>
   
  </div>

 </div>
   
  </body>
</html>
